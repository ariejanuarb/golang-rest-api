{
  "openapi": "3.0.3",
  "info": {
    "title": "Category API",
    "description": "API for CRUD category",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000/api"
    }
  ],
  "paths": {
    "/categories": {
      "get": {
        "security": [{
          "CategoryAuth" : []
        }],
        "tags": [
          "Category API"
        ],
        "description": "List all Categories",
        "summary": "List all Categories",
        "responses": {
          "200": {
            "description": "Success get all categories",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "number"
                    },
                    "status": {
                      "type": "string"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Category"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
